    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "enzyme": "^2.6.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
